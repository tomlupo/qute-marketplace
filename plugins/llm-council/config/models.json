{
  "models": [
    {
      "name": "gpt",
      "provider": "openai",
      "model": "gpt-5.2",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "api_key_env": "OPENAI_API_KEY",
      "cli_command": "codex",
      "enabled": true,
      "description": "OpenAI GPT-5.2 - latest flagship"
    },
    {
      "name": "gemini",
      "provider": "google",
      "model": "gemini-3",
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-3:generateContent",
      "api_key_env": "GOOGLE_API_KEY",
      "cli_command": "gemini",
      "enabled": true,
      "description": "Google Gemini 3 - latest generation"
    },
    {
      "name": "claude",
      "provider": "anthropic",
      "model": "claude-opus-4-5-20251101",
      "endpoint": "https://api.anthropic.com/v1/messages",
      "api_key_env": "ANTHROPIC_API_KEY",
      "cli_command": "claude",
      "enabled": true,
      "description": "Anthropic Claude Opus 4.5 - latest flagship"
    },
    {
      "name": "deepseek",
      "provider": "deepseek",
      "model": "deepseek-reasoner",
      "endpoint": "https://api.deepseek.com/chat/completions",
      "api_key_env": "DEEPSEEK_API_KEY",
      "cli_command": null,
      "enabled": true,
      "description": "DeepSeek R1 - exceptional reasoning (API only, no CLI)"
    }
  ],
  "settings": {
    "max_tokens": 4096,
    "temperature": 0.7,
    "timeout_seconds": 180
  }
}
